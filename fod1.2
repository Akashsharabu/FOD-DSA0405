import pandas as pd

data = {
    "cust_id" : [1,1,1,2,3,3],
    'order_date': ['2023-01-01', '2023-01-02', '2023-01-03', '2022-12-31', '2023-01-02', '2023-01-04'],
    "prdt_name" : ["A","A","B","B","C","C"],
    "order_quantity" : [10,11,15,11,2,3]
    }
order_data = pd.DataFrame(data)
total_orders_per_customer = order_data.groupby('cust_id')['order_date'].count()

average_order_quantity_per_product = order_data.groupby('prdt_name')['order_quantity'].mean()

earliest_order_date = order_data['order_date'].min()
latest_order_date = order_data['order_date'].max()

print("1. Total number of orders made by each customer:")
print(total_orders_per_customer)

print("\n2. Average order quantity for each product:")
print(average_order_quantity_per_product)

print("\n3. Earliest and Latest order dates in the dataset:")
print("Earliest Order Date:", earliest_order_date)
print("Latest Order Date:", latest_order_date)
